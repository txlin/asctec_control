<launch>

	<node pkg="asctec_so3"
		type="asctec_control_node"
		name="asctec_so3_control"
		required="true"
		output="screen">

		<param name="mass" value="0.55"/>
		<param name="thrust" value="26.683"/>
		<rosparam param="gains/pos">[6.4, 6.4, 8.3]</rosparam>   
		<!-- Normal 5.5, 5.5, 8.2 --> 
		<!-- Aggressive 6.4, 6.4, 8.2 -->

		<rosparam param="gains/vel">[5.5, 5.5, 4.45]</rosparam>   
		<!-- Normal 4.6, 4.6, 4.5 -->
		<!-- Aggressive 5.0, 5.0, 4.6 -->

		<rosparam param="gains/yaw">0.6</rosparam> <!-- 1.3-->
		<rosparam param="gains/yaw_dot">0.3</rosparam> <!-- 1.3-->
	</node>

</launch>
