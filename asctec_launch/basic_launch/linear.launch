<launch>
		<node pkg="asctec_lin"
		  type="asctec_lin_node"
		  name="asctec_lin_control"
		  required="true"
		  output="screen">
		  <param name="mass" value="0.55"/>
		  <rosparam param="gains/pos">[0.5, 0.5, 6]</rosparam>
		  <rosparam param="gains/vel">[0.5, 0.5, 4.95]</rosparam>
		  <rosparam param="gains/int">[0.005, 0.005, 0.25]</rosparam>
			<rosparam param="gains/kiyaw">0.0</rosparam>
			<rosparam param="gains/kyaw">0.5</rosparam>
		</node>
</launch>
